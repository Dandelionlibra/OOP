package homework1;

import java.util.InputMismatchException;
import java.util.Scanner;

public class HW3 {

	public static void main(String[] args){

		for(int i = 0 ; i < 5 ; i++) {
			for(int j = 0 ; j < 7 ; j++) {
				if(i == j)
					System.out.print("*");
				else
					System.out.print(" ");
				
			}
			System.out.print("\n");
		}
		return;
	}
}
