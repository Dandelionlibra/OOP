package homework1;

public class HW4 {

	
	public static void main(String[] args){
		System.out.println("# This is the fourth question #");
		for(int i = 1 ; i < 5 ; i++) {
			for(int j = 1 ; j < 5-i ; j++)
				System.out.print(" ");
			
			for(int j = 1 ; j < 2*i ; j++)
				System.out.print("*");
			
			System.out.print("\n");
		}
		return;
	}
}
